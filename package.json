{
  "name": "expo-detox-branch",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "prebuild": "npx expo prebuild --clean",
    "build:ios:sim": "EXPO_DEBUG=1 npx eas build -p ios --profile simulator",
    "build:ios:dev": "EXPO_DEBUG=1 npx eas build -p ios --profile development",
    "build:ios:prod": "EXPO_DEBUG=1 npx eas build -p ios --profile production",
    "build:android:emu": "EXPO_DEBUG=1 npx eas build -p android --profile emulator",
    "build:android:dev": "EXPO_DEBUG=1 npx eas build -p android --profile development",
    "build:android:prod": "EXPO_DEBUG=1 npx eas build -p android --profile production",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "detox:build:ios:debug": "npx detox build -c ios.sim.debug",
    "detox:build:ios:release": "npx detox build -c ios.sim.release",
    "detox:build:android:debug": "npx detox build -c android.emu.debug",
    "detox:build:android:release": "npx detox build -c android.emu.release",
    "detox:test:ios:debug": "npx detox test -c ios.sim.debug",
    "detox:test:ios:watch": "npx detox test -c ios.sim.debug --reuse --watchAll",
    "detox:test:ios:release": "npx detox test -c ios.sim.release",
    "detox:test:android:debug": "npx detox test -c android.emu.debug",
    "detox:test:android:watch": "npx detox test -c android.emu.debug --reuse --watchAll",
    "detox:test:android:release": "npx detox test -c android.emu.release",
    "detox:test:android:attached": "npx detox test -c android.att.release",
    "detox:ios:debug": "yarn detox:build:ios:debug && yarn detox:test:ios:debug",
    "detox:ios:watch": "yarn detox:build:ios:debug && yarn detox:test:ios:watch",
    "detox:ios:release": "yarn detox:build:ios:release && yarn detox:test:ios:release",
    "detox:android:debug": "yarn detox:build:android:debug && yarn detox:test:android:debug",
    "detox:android:watch": "yarn detox:build:android:debug && yarn detox:test:android:watch",
    "detox:android:release": "yarn detox:build:android:release && yarn detox:test:android:release",
    "detox:android:attached": "yarn detox:build:android:release && yarn detox:test:android:attached"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/config-plugins": "~7.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/native": "^6.0.2",
    "expo": "~49.0.15",
    "expo-constants": "14.5.0",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@config-plugins/detox": "^6.0.0",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.14",
    "babel-jest": "^29.7.0",
    "detox": "^20.14.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "~49.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "private": true
}
